    "%%time\n",
    "#selection sort\n",
    "def SelectionSort(A):\n",
    "    \n",
    "    for i in range(len(A)):\n",
    "        min_elem_index = i\n",
    "        \n",
    "        for j in range(i + 1 ,len(A)):\n",
    "            if A[j] < A[min_elem_index]:\n",
    "                min_elem_index = j\n",
    "        A[i], A[min_elem_index] = A[min_elem_index],A[i]\n",
    "        \n",
    "    return A\n",
    "\n",
    "#example\n",
    "A = new_array()\n",
    "print(SelectionSort(A))"
